{
  "name": "nuxt-app",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "db:generate": "drizzle-kit generate:mysql --config=drizzle.config.ts",
    "db:migrate": "ts-node ./server/database migrate",
    "db:push": "drizzle-kit push:mysql --config=drizzle.config.ts",
    "db:drop": "drizzle-kit drop --config=drizzle.config.ts",
    "lint": "eslint . --ext .js,.ts,.vue",
    "lint:fix": "eslint . --fix"
  },
  "devDependencies": {
    "@nuxt/devtools": "latest",
    "@nuxtjs/eslint-config-typescript": "^12.1.0",
    "@nuxtjs/tailwindcss": "^6.11.3",
    "@vueuse/core": "^10.7.2",
    "@vueuse/nuxt": "^10.7.2",
    "drizzle-kit": "^0.20.14",
    "eslint": "^8.56.0",
    "nuxt": "^3.10.0",
    "sass": "^1.70.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@he-tree/vue": "^2.8.1",
    "drizzle-orm": "^0.29.3",
    "mysql2": "^3.9.1",
    "nanoid": "^4.0.2",
    "nuxt-swiper": "^1.2.2",
    "zod": "^3.22.4"
  }
}