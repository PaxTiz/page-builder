{
  "name": "nuxt-app",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "db:generate": "drizzle-kit generate:mysql --config=drizzle.config.ts",
    "db:migrate": "ts-node ./server/database migrate",
    "db:push": "drizzle-kit push:mysql --config=drizzle.config.ts",
    "db:drop": "drizzle-kit drop --config=drizzle.config.ts",
    "lint": "eslint . --ext .js,.ts,.vue",
    "lint:fix": "eslint . --fix"
  },
  "devDependencies": {
    "@nuxt/devtools": "latest",
    "@nuxtjs/eslint-config-typescript": "^12.0.0",
    "@types/node": "^18.17.0",
    "@unocss/nuxt": "^0.55.0",
    "@unocss/transformer-directives": "^0.53.6",
    "@vueuse/core": "^10.2.1",
    "@vueuse/nuxt": "^10.2.1",
    "eslint": "^8.46.0",
    "nuxt": "^3.10.0",
    "sass": "^1.64.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6",
    "drizzle-kit": "^0.20.14"
  },
  "dependencies": {
    "@he-tree/vue": "^2.4.2",
    "drizzle-orm": "^0.29.3",
    "mysql2": "^3.6.0",
    "nanoid": "^4.0.2",
    "nuxt-swiper": "^1.2.0",
    "zod": "^3.21.4"
  }
}